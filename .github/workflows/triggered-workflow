name: Workflow triggered workflow
on:
  workflow_run:
    workflow:["Manually triggered workflow"]
    types
	  - completed

jobs:
  wf-triggered:
    name: Work Flow Triggered
	branches: [test]
    runs-on: ubuntu-latest
    steps:
      - run: |
          echo "Hello ${{ github.event.inputs.name }}!"
          echo "- in ${{ github.event.inputs.home }}!"
